{
  "name": "FreeSurfer Post Report and Overlays",
  "authors": "Rudolph Pienaar<rudolph.pienaar@childrens.harvard.edu",
  "category": "Medical Imaging",
  "description": "Once a FreeSurfer analysis is complete, this pipeline generates reports and also some reference overlay images of the segmentations superimposed on the original image. Attach this pipeline to a completed FreeSurfer analysis node.",
  "locked":	true,
  "plugin_tree": [
    {
      "plugin_name": "pl-simpledsapp",
      "plugin_version": "2.1.0",
      "previous_index": null
    },  
    {
      "plugin_name": "pl-mgz2lut_report",
      "plugin_version": "1.3.2",
      "previous_index": 0,
      "plugin_parameter_defaults": [
        {
            "name":         "file_name",
            "default":      "FreeSurferRecon/mri/aparc.a2009s+aseg.mgz"
        },
        {
            "name":         "report_name",
            "default":      "volume_segmentation_report"
        },
        {
            "name":         "report_types",
            "default":      "txt,csv,json,html.pdf"
        },
        {
            "name":         "LUT",
            "default":      "FreeSurferColorLUT.txt"
        }
      ]
    },
    {
      "plugin_name": "pl-multipass",
      "plugin_version": "1.3.0",
      "previous_index": 0,
      "plugin_parameter_defaults": [
        {
            "name":         "commonArgs",
            "default":      "'--printElapsedTime --verbosity 5 --saveImages --skipAllLabels --outputFileStem sample --outputFileType png'"
        },
        {
            "name":         "specificArgs",
            "default":      "'--inputFile FreeSurferRecon/mri/brainmask.mgz --wholeVolume brainVolume ++ --inputFile FreeSurferRecon/mri/aparc.a2009s+aseg.mgz --wholeVolume segVolume --lookupTable __fs__'"
        },
        {
            "name":         "splitExpr",
            "default":      "++"
        },
        {
            "name":         "exec",
            "default":      "pfdo_mgz2image"
        },
        {
            "name":         "noJobLogging",
            "default":      true
        },
        {
            "name":         "verbose",
            "default":      "1"
        }
      ]
    },
    {
        "plugin_name": "pl-pfdorun",
        "plugin_version": "2.2.7",
        "previous_index": 2,
        "plugin_parameter_defaults": [
            {
                "name":         "dirFilter",
                "default":      "label-brainVolume"
            },
            {
                "name":         "fileFilter",
                "default":      "png"
            },
            {
                "name":         "analyzeFileIndex",
                "default":      "-1"
            },
            {
                "name":         "exec",
                "default":      "'composite -dissolve 90 -gravity Center %inputWorkingDir/%inputWorkingFile %inputWorkingDir/../../aparc.a2009s+aseg.mgz/label-segVolume/%inputWorkingFile -alpha Set %outputWorkingDir/%inputWorkingFile'"
            },
            {
                "name":         "noJobLogging",
                "default":      true
            },
            {
                "name":         "verbose",
                "default":      "5"
            }        
        ]
    }
  ]
}
