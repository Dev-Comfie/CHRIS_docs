{
  "name": "FreeSurfer",
  "authors": "Rudolph Pienaar<rudolph.pienaar@childrens.harvard.edu",
  "category": "Medical Imaging",
  "description": "Run FreeSurfer (recon -all) across an input data space (can be multidir) -- no mess, no fuss",
  "locked":	false,
  "plugin_tree": [
    {
      "plugin_name": "pl-fshack",
      "plugin_version": "1.4.1",
      "previous_index": null,
      "plugin_parameter_defaults": [
        {
            "name":         "args",
            "default":      "ARGS: -all"
        },
        {
            "name":         "exec",
            "default":      "recon-all"
        },
        {
            "name":         "inputFile",
            "default":      ".dcm"
        },
        {
            "name":         "outputFile",
            "default":      "FreeSurferRecon"
        }
      ]
    }       
  ]
}
